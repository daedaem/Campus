<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.project.dao.SnsReplyDao">

    <insert id="snsReplyInsert" parameterType="com.ssafy.project.dto.SnsReplyDto" useGeneratedKeys="true" keyProperty="snsReplyNo">
        INSERT INTO SNSREPLY ( snsNo, userNo, snsReplyContent, snsReplyCreateTime )
                        VALUES ( #{snsNo}, #{userNo}, #{snsReplyContent}, now() )
    </insert>

    <select id="snsReplyList" parameterType="com.ssafy.project.dto.SnsReplyDto">
        SELECT *
            FORM SNSREPLY
    <select